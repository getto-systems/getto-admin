{
  "name": "getto-css",
  "version": "0.4.0",
  "description": "simple admin css theme",
  "scripts": {
    "start": "npm run reload & npm run watch",
    "reload": "reload-wrapper -d ./public",
    "watch": "watch 'npm run build' src",
    "build": "postcss src/getto.css --dir public/dist"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/getto-systems/getto-css.git"
  },
  "author": "shun@getto.systems",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/getto-systems/getto-css/issues"
  },
  "homepage": "https://github.com/getto-systems/getto-css#readme",
  "devDependencies": {
    "autoprefixer": "^8.6.5",
    "csswring": "^6.0.3",
    "getto-detect": "^0.1.0",
    "pixrem": "^3.0.2",
    "postcss-apply": "^0.9.0",
    "postcss-calc": "^6.0.1",
    "postcss-cli": "^5.0.1",
    "postcss-color-gray": "^4.1.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-import": "^9.1.0",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^5.0.0",
    "postcss-utilities": "^0.6.1",
    "reload-wrapper": "^0.1.1",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "watch": "^1.0.2"
  }
}
