{
  "name": "getto-css",
  "version": "2.1.3",
  "description": "simple admin css theme",
  "scripts": {
    "start": "npm run prepare:development && npm run start:webpack",
    "start:webpack": "webpack-dev-server --config ./webpack.config.js",
    "build": "npm run prepare:production && npm run build:all",
    "build:all": "npm run build:webpack && npm run build:postcss",
    "build:webpack": "webpack --config ./webpack.config.js --mode production",
    "build:postcss": "postcss --config ./postcss.config.js --output css/getto.css src/getto.css",
    "prepare:development": "node scripts/prepare.js --development",
    "prepare:production": "node scripts/prepare.js --production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/getto-systems/getto-css.git"
  },
  "author": "shun@getto.systems",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/getto-systems/getto-css/issues"
  },
  "homepage": "https://github.com/getto-systems/getto-css#readme",
  "devDependencies": {
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "htm": "^3.0.4",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "preact": "^10.4.4",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
